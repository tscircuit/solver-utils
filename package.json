{
  "name": "@tscircuit/solver-utils",
  "main": "dist/index.js",
  "version": "0.0.9",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./react": "./dist/react/index.js"
  },
  "type": "module",
  "scripts": {
    "start": "cosmos",
    "build": "tsup-node ./lib/index.ts --format esm --dts -d dist && tsup-node ./lib/react/index.ts --format esm --dts -d dist/react",
    "build:site": "cosmos-export",
    "format": "biome format --write .",
    "format:check": "biome format ."
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@react-hook/resize-observer": "^2.0.2",
    "@types/bun": "latest",
    "@types/react": "^19.1.13",
    "graphics-debug": "^0.0.76",
    "react": "^19.1.1",
    "react-cosmos": "^7.0.0",
    "react-cosmos-plugin-vite": "^7.0.0",
    "react-dom": "^19.1.1",
    "tsup": "^8.5.0",
    "vite": "^7.3.0"
  },
  "peerDependencies": {
    "typescript": "^5",
    "graphics-debug": "*"
  }
}
